<NotepadPlus>
    <UserLang name="abjad" ext="abjad" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="no" foldCompact="no" forcePureLC="0" decimalSeparator="0" />
        </Settings>
        <KeywordLists>
            <Keywords name="Comments">03comm</Keywords>
            <Keywords name="Keywords1">def asgn import asrt dbg inp out cout nop</Keywords>
            <Keywords name="Keywords2">e j pi tau eta inf eps mu omg rand</Keywords>
            <Keywords name="Keywords3">add sub mul div mod pow log</Keywords>
            <Keywords name="Keywords4">sin cos tan asin acos atan sqrt abs sgn rcp neg flr ceil rnd</Keywords>
            <Keywords name="Keywords5">eq neq lt lte gt gte min max</Keywords>
            <Keywords name="Keywords6">dup pop swp lft rgt </Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" fgColor="FFFFFF" bgColor="000000" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="COMMENTS" fgColor="808080" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="NUMBERS" fgColor="FF8040" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS1" fgColor="FF8080" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS2" fgColor="80FFFF" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS3" fgColor="FFFF80" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS4" fgColor="FF8000" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS5" fgColor="FF80FF" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
            <WordsStyle name="KEYWORDS6" fgColor="80FF80" bgColor="FFFFFF" colorStyle="1" fontStyle="0" nesting="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
